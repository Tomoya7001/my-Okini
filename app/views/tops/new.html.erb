<h1>Top#new</h1>
<p>Find me in app/views/top/new.html.erb</p>

<%= form_with model: @top, local: true do |f| %>

  <div class="field">
    <%= f.label :video %>
    <%= f.file_field :video_top, :accept => 'video/*' %>
  </div>

  <div class="#">
    <%= f.submit "作成する" ,class:"#" %>
    <%=link_to 'もどる', root_path, class:"back-btn" %>
  </div>

<% end %>
